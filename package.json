{
  "name": "users-service-api",
  "version": "0.0.0",
  "description": "Users api with Express, TypeORM and JWT",
  "main": "server.ts",
  "scripts": {
    "start": "node dist/main/server.js",
    "start:debug": "node --inspect --require ts-node/register src/main/server.ts",
    "build": "rimraf dist/ && npx tsc",
    "dev": "nodemon src/main/server.ts",
    "test": "vitest --passWithNoTests --silent",
    "test:unit": "npm test -- run -c vitest.unit.config.ts",
    "test:integration": "npm test -- run -c vitest.integration.config.ts",
    "test:watch": "npm test -- --watch",
    "test:verbose": "vitest --passWithNoTests",
    "test:staged": "npm test -- related",
    "test:coverage": "npm test -- run --coverage",
    "prepare": "husky",
    "prettier": "npx prettier --config .prettierrc --write \"src/**/*.ts\""
  },
  "keywords": [
    "SOLID",
    "Express",
    "Node",
    "Clean",
    "Architecture"
  ],
  "author": "Raniel Sobrinho",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.7",
    "@types/pg": "^8.11.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vitest/coverage-v8": "^1.5.0",
    "dotenv": "^16.4.5",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "git-commit-msg-linter": "^5.0.7",
    "globals": "^15.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.2.5",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.0"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "pg": "^8.11.5",
    "rimraf": "^5.0.5"
  },
  "_moduleAliases": {
    "@": "dist/"
  }
}
